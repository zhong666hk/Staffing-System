<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wbu.staff.employee.mapper.EmployeeMapper">

    <resultMap id="BaseResultMap" type="com.wbu.staff.employee.domain.Employee">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="mobile" column="mobile" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="isDelete" column="is_delete" jdbcType="TINYINT"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="workId" column="work_id" jdbcType="CHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,mobile,password,
        create_time,is_delete,name,
        work_id
    </sql>
</mapper>
